# Local Machine Definition file for pysiral
# (this file is ignored by git)
---

# Definition of the local machine
local_machine:
    id: 'MrOrange'
    n_processors: 1


# Default data output for all products generated by pysiral
# The data will be organized as:
#   $data_repository_product / $mission / $jobid / Year / (Month or Week)
data_repository_product: 'E:\pysiral_product'


# Radar altimeter input data for different missions
local_l1b_repository:
    # CryoSat-2
    cryosat2:
        sar: 'E:\sat-data\cs2\SIR_SAR_L1'
        sin: 'E:\sat-data\cs2\SIR_SIN_L1'


# Auxiliary data sets
# The tags of the auxdata datasets must match those in `auxdata_def.yaml`
local_auxdata_repository:

    # Mean sea surface height products
    mss:
        dtu_mss: 'E:\cs2awi\cryosat_seaice\auxdata\mss'
